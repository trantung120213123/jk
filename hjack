            // Toggle panel minimize
            $("#togglePanel").on('click', function(){
                isPanelMinimized = !isPanelMinimized;
                if(isPanelMinimized) {
                    $("#personalDiv").addClass('panel-minimized');
                    $(this).text('M·ªü r·ªông');
                } else {
                    $("#personalDiv").removeClass('panel-minimized');
                    $(this).text('Thu g·ªçn');
                }
            });

            // C·∫≠p nh·∫≠t delay khi ng∆∞·ªùi d√πng nh·∫≠p
            $("#minDelay").on('change', function(){
                minAutoDelay = parseFloat(this.value);
                if (minAutoDelay < 0.1) minAutoDelay = 0.1;
                if (minAutoDelay > maxAutoDelay) {
                    minAutoDelay = maxAutoDelay - 0.1;
                    this.value = minAutoDelay;
                }
                localStorage.setItem('krypbot_minDelay', minAutoDelay);
                updateDelayDisplay();
            });

            $("#maxDelay").on('change', function(){
                maxAutoDelay = parseFloat(this.value);
                if (maxAutoDelay < 0.1) maxAutoDelay = 0.1;
                if (maxAutoDelay < minAutoDelay) {
                    maxAutoDelay = minAutoDelay + 0.1;
                    this.value = maxAutoDelay;
                }
                localStorage.setItem('krypbot_maxDelay', maxAutoDelay);
                updateDelayDisplay();
            });

            // C·∫≠p nh·∫≠t hi·ªÉn th·ªã ban ƒë·∫ßu
            updateDelayDisplay();

            $("#personalDiv").on('click',function(e){
            if(e.target.tagName == "INPUT")
            {
                const type_name = e.target.name;
                const type_value = Number.parseInt(e.target.value);
                switch(type_name){
                    case "show-eval":
                        show_evaluation = type_value?true:false
                        if(type_value)
                        {
                            $('#evaluation').css({'display':'block'})
                        }else{
                        $('#evaluation').css({'display':'none'})}
                        break;

                    case "bot-status":
                        chessBot.status = type_value
                        if(type_value){
                            hint = true
                        }
                        else
                        {
                            hint = false
                            $(".myhigh").remove()
                        }
                    break;
                    case "bot-move":
                        auto_move = type_value?true:false
                        break;
                    case "bot-nature":
                        chessBot.nature = type_value
                    break;
                    case "bot-type":
                        chessBot.type = type_value
                    break;

                    default:
                        console.log('none')
                }
            }
            })

            $("#eloRange").on('change',function(){
                chessBot.elo = Number.parseInt(this.value)
                $('#eloShow').text("ƒêang ch∆°i ·ªü ELO " + chessBot.elo)
            })

            $("#colorPicker").on('change',function(){
                current_color = this.value
            })

             interval = setInterval(()=>{if(main_interval){get_hint()}},100)
        });
    };
})();
