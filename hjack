(function() {
    'use strict';
    let interval,show_opponent=false,can_interval = true,main_interval=true,show_evaluation=true,
        auto_move,current_color = '#000000',fen,checkfen,cp=0,best_cp=0,hint=false,username, Messages = [],msgLen=0;
    let chessBot = {elo:3200,power:15,status:0,nature:1,type:1,fen:0,time:0.3}
    let pendingAutoMove = false;
    let isPanelMinimized = false;

    // C√†i ƒë·∫∑t m·∫∑c ƒë·ªãnh cho auto move delay
    let minAutoDelay = 4; // Gi√¢y
    let maxAutoDelay = 20; // Gi√¢y

    if(!localStorage.getItem('username'))
    {
        username = 'User' + [...Array(9).keys()]
  .map(n => n + 1)
  .sort(() => Math.random() - 0.5)
  .slice(0, 5).join('');
    }
    else
    {
        username = localStorage.getItem('username')
    }

    // Load c√†i ƒë·∫∑t t·ª´ localStorage n·∫øu c√≥
    if(localStorage.getItem('krypbot_minDelay')) {
        minAutoDelay = parseInt(localStorage.getItem('krypbot_minDelay'));
    }
    if(localStorage.getItem('krypbot_maxDelay')) {
        maxAutoDelay = parseInt(localStorage.getItem('krypbot_maxDelay'));
    }

    const script = document.createElement('script');

    script.setAttribute('crossorigin', 'anonymous');
    script.setAttribute('src', 'https://code.jquery.com/jquery-3.7.1.js');
    script.setAttribute('integrity', 'sha256-eKhayi8LEQwp4NKxN+CfCh+3qOVUtJn3QNZ0TciWLP4=');
    script.setAttribute('crossorigin', 'anonymous');
    document.body.appendChild(script);

    script.onload = () => {
        $('<link>', {
    rel: 'stylesheet',
    type: 'text/css',
    href: 'https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Roboto:wght@400;500;700&display=swap'
}).appendTo('head');

        console.log('jQuery loaded!');
        $(document).ready(function() {
            const get_number = (elm) =>{
             const data = ['a','b','c','d','e','f','g','h']
             return data.indexOf(elm)+1
            }

            const create_elm = (num)=>{
                const board = $('chess-board')[0] || $('wc-chess-board')[0] || document.querySelector('.chessboard');
                const turn = board?.game?.getTurn ? (board.game.getTurn() === board.game.getPlayingAs()) : true;
                const elm = document.createElement('div')
             elm.setAttribute('class',`highlight square-${num} myhigh`)
             const jelm = $(elm).css({'opacity':'0','border':`4px solid ${current_color}`,'background':'rgba(15, 10, 222,0.4)','shadow':'0 0 10px rgba(3, 201, 169,0.8)','border-radius':'50%'})

             // Th√™m v√†o board ph√π h·ª£p
             if($('#board-play-computer').length) $('#board-play-computer').append(jelm)
             else if($('#board-single').length) $('#board-single').append(jelm)
             else if($('.chessboard').length) $('.chessboard').append(jelm)

             const x = jelm.position().left
             const y = jelm.position().top;
             const w = jelm.outerWidth();
             const h = jelm.outerHeight();

             return [(x+w+x)/2, (y+h+y)/2];
            }

         const auto_move_piece = function(from, to,board){
        for (var each=0;each<board.game.getLegalMoves().length;each++){
            if(board.game.getLegalMoves()[each].from == from){
                if(board.game.getLegalMoves()[each].to == to){
                    var move = board.game.getLegalMoves()[each];
                    board.game.move({
                        ...move,
                        promotion: 'false',
                        animate: false,
                        userGenerated: true
                    });
                }
            }
        }
    }

            // H√†m l·∫•y delay ng·∫´u nhi√™n v·ªõi ph·∫°m vi c√≥ th·ªÉ c·∫•u h√¨nh
            const getRandomDelay = () => {
                if (minAutoDelay < 0.1) minAutoDelay = 0.1; // Gi·ªõi h·∫°n t·ªëi thi·ªÉu 0.1 gi√¢y
                if (maxAutoDelay < minAutoDelay) maxAutoDelay = minAutoDelay + 1;

                return (Math.random() * (maxAutoDelay - minAutoDelay) + minAutoDelay) * 1000;
            }

            // H√†m c·∫≠p nh·∫≠t hi·ªÉn th·ªã ph·∫°m vi delay
            const updateDelayDisplay = () => {
                $('#delayShow').text(`T·ª± ƒë·ªông ƒëi sau ${minAutoDelay}-${maxAutoDelay} gi√¢y`);
            }

            // H√†m l·∫•y FEN cho ChessGPT.vn
            const getChessGPTFEN = () => {
                try {
                    // C·ªë g·∫Øng l·∫•y FEN t·ª´ nhi·ªÅu ngu·ªìn kh√°c nhau

                    // Ph∆∞∆°ng √°n 1: T√¨m trong window object
                    if(window.chessGame && window.chessGame.fen) {
                        return window.chessGame.fen();
                    }

                    // Ph∆∞∆°ng √°n 2: T√¨m t·ª´ React props
                    const boardElement = document.querySelector('.chessboard');
                    if(boardElement) {
                        for(let key in boardElement) {
                            if(key.startsWith('__react')) {
                                const props = boardElement[key];
                                if(props?.memoizedProps?.position) {
                                    return convertPositionToFEN(props.memoizedProps.position);
                                }
                            }
                        }
                    }

                    // Ph∆∞∆°ng √°n 3: ƒê·ªçc tr·ª±c ti·∫øp t·ª´ DOM
                    return parseBoardToFEN();

                } catch(e) {
                    console.log('L·ªói khi l·∫•y FEN t·ª´ ChessGPT:', e);
                    return null;
                }
            }

            // Parse board DOM th√†nh FEN
            const parseBoardToFEN = () => {
                const squares = document.querySelectorAll('.chessboard .square');
                if(squares.length !== 64) return null;

                let fen = '';
                let emptyCount = 0;

                for(let i = 0; i < 64; i++) {
                    const square = squares[i];
                    const piece = square.querySelector('[class*="piece"]');

                    if(piece) {
                        if(emptyCount > 0) {
                            fen += emptyCount;
                            emptyCount = 0;
                        }

                        const classList = piece.className;
                        let pieceChar = '';

                        if(classList.includes('pawn')) pieceChar = 'p';
                        else if(classList.includes('knight')) pieceChar = 'n';
                        else if(classList.includes('bishop')) pieceChar = 'b';
                        else if(classList.includes('rook')) pieceChar = 'r';
                        else if(classList.includes('queen')) pieceChar = 'q';
                        else if(classList.includes('king')) pieceChar = 'k';

                        if(classList.includes('white')) pieceChar = pieceChar.toUpperCase();

                        fen += pieceChar;
                    } else {
                        emptyCount++;
                    }

                    if((i + 1) % 8 === 0) {
                        if(emptyCount > 0) {
                            fen += emptyCount;
                            emptyCount = 0;
                        }
                        if(i < 63) fen += '/';
                    }
                }

                return fen + ' w KQkq - 0 1'; // Th√™m c√°c th√¥ng tin m·∫∑c ƒë·ªãnh
            }

            // H√†m th·ª±c hi·ªán n∆∞·ªõc ƒëi cho ChessGPT
            const makeChessGPTMove = (from, to) => {
                try {
                    // T√¨m √¥ xu·∫•t ph√°t v√† ƒë√≠ch
                    const fromSquare = document.querySelector(`[data-square="${from}"]`);
                    const toSquare = document.querySelector(`[data-square="${to}"]`);

                    if(fromSquare && toSquare) {
                        // Simulate drag and drop
                        const dragStartEvent = new MouseEvent('mousedown', {
                            bubbles: true,
                            cancelable: true,
                            clientX: fromSquare.getBoundingClientRect().left + 20,
                            clientY: fromSquare.getBoundingClientRect().top + 20
                        });

                        const dragEndEvent = new MouseEvent('mouseup', {
                            bubbles: true,
                            cancelable: true,
                            clientX: toSquare.getBoundingClientRect().left + 20,
                            clientY: toSquare.getBoundingClientRect().top + 20
                        });

                        fromSquare.dispatchEvent(dragStartEvent);
                        setTimeout(() => toSquare.dispatchEvent(dragEndEvent), 100);
                    } else {
                        console.log('Kh√¥ng t√¨m th·∫•y √¥ c·ªù tr√™n ChessGPT');
                    }
                } catch(e) {
                    console.log('L·ªói khi th·ª±c hi·ªán n∆∞·ªõc ƒëi tr√™n ChessGPT:', e);
                }
            }

            const create_div = (str1) =>{
                try{
                const target = $('chess-board')[0] || $('wc-chess-board')[0] || document.querySelector('.chessboard');

                const a = get_number(str1[0])
                const b = get_number(str1[2])
                console.log(str1.substring(0,2),str1.substring(2,4))

                if(auto_move && !pendingAutoMove){
                    pendingAutoMove = true;
                    const randomDelay = getRandomDelay();
                    console.log(`Auto move s·∫Ω th·ª±c hi·ªán sau ${(randomDelay/1000).toFixed(1)} gi√¢y`);

                    setTimeout(() => {
                        const board = $('chess-board')[0] || $('wc-chess-board')[0];
                        if(board && board.game) {
                            auto_move_piece(str1.substring(0,2),str1.substring(2,4), board)
                        } else {
                            // X·ª≠ l√Ω cho chessgpt.vn
                            makeChessGPTMove(str1.substring(0,2),str1.substring(2,4));
                        }
                        pendingAutoMove = false;
                    }, randomDelay);
                }

                const first_elm = create_elm(a+str1[1])
                const last_element = create_elm(b+str1[3])

                if (target) {
                    $(target).append(`
        <svg width="100%" height="100%" class='myhigh' style="position: absolute; top: 0; left: 0; pointer-events: none;">
          <defs>
            <marker id="arrowhead" markerWidth="12" markerHeight="10"
                    refX="10" refY="3.5" orient="auto">
              <polygon points="0 0, 10 3.5, 0 7" fill=${current_color} />
            </marker>
          </defs>
          <line x1="${first_elm[0]}" y1="${first_elm[1]}" x2="${last_element[0]}" y2="${last_element[1]}"
                stroke=${current_color} stroke-width="4" marker-end="url(#arrowhead)" />
        </svg>
    `);
                }
                }
                catch(e){
                    console.log("an error has occured", e)
                }
            }

             async function get_hint()
            {
                main_interval = false
                try{
                let continuation

                    $('.my-high').remove()

                const board = $('chess-board')[0] || $('wc-chess-board')[0];

const len = $('.myhigh').length
const opp_len = $('hishigh').length
const my_peice = board.game.getPlayingAs()
const turn = board.game.getTurn()
fen = board.game.getFEN()
chessBot.fen = board.game.getFEN()

                if(board.game.getTurn() ==  board.game.getPlayingAs())
                {
                    $(".myanalysis").remove()

                    if(checkfen !== fen)
                    {
                        console.log("am right there")
                        try{
                        checkfen = fen;
                        const data = await fetch(`https://sanandre.pythonanywhere.com/bestCp/?fen=${fen}&turn=${my_peice}`)
                        const resp = await data.json();
                            console.log(resp)
                        best_cp = resp.best_cp;
                    }
                     catch(e){
                     console.log('some error occured again during best_cp'+e)
                     }
                    }

                if(!len && can_interval &&hint){
                 can_interval = false
                 try{
                    console.log("nice");
                     const data = await fetch(`https://sanandre.pythonanywhere.com/getMove/`, {
                         method: "POST",
                         headers: {
                             "Content-Type": "application/json"
                         },
                         body: JSON.stringify(chessBot)
                     });
                const resp = await data.json();
                continuation = resp[0]
                    console.log(continuation)

                  create_div(continuation)

                    can_interval=true
                }

              catch(e)
              {
                  console.log("an error has cocured" + e);can_interval=true
              }
                }
               }
               else{
                    $('.myhigh').remove()
                    if(fen !== checkfen)
                    {
                        const lastMove = board.game.getLastMove().to
                        const finalAalysisMove = String(get_number(lastMove[0])+String(lastMove[1]))
                        console.log(lastMove,finalAalysisMove)

                        checkfen=fen;
                        try
                        {
                        const data = await fetch(`https://sanandre.pythonanywhere.com/getCp?fen=${encodeURIComponent(fen)}&turn=${my_peice}&cp=${cp}&best_cp=${best_cp}`)
                        const resp = await data.json()

                        cp = resp.cp;
                            $('.myanalysis').remove()
                            if(show_evaluation){
                        const img_moves = resp.img;

                        $(board).append(`<div class='myanalysis highlight square-${finalAalysisMove}'  data-test-element='highlight' style=';background-color:transparent' >
                 <img style="position:absolute;top:0;width:20px;height:20x"
                 src=${img_moves}>
                 </div>`)
                            }
                        $('#evalPosition').text(resp.winning);
                        $('#evalMove').text(resp.value);
                        $('#evalMove').css({ "color": resp.color });
                        }
                        catch(e){
                            console.log('some error occured again during cp' +e)
                        }
                    }
               }
                }
                catch(e)
                {
                    main_interval=true
                }
                main_interval = true
            }

            const main_div = $('#board-layout-main, .game-container, main').first();
main_div.append(`
